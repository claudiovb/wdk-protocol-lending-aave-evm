{
  "name": "@tetherto/wdk-protocol-lending-aave-evm",
  "version": "1.0.0-beta.2",
  "description": "A simple package that lets @tetherto/wdk-wallet-evm and @tetherto/wdk-wallet-evm-erc-4337 wallet accounts interact with the aave protocol.",
  "keywords": [
    "wdk",
    "protocol",
    "lending",
    "aave",
    "ethereum",
    "erc-4337"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/tetherto/wdk-protocol-lending-aave-evm.git"
  },
  "main": "index.js",
  "type": "module",
  "types": "./types",
  "scripts": {
    "build:types": "tsc",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "dependencies": {
    "@bgd-labs/aave-address-book": "4.33.0",
    "@tetherto/wdk-wallet": "^1.0.0-beta.1",
    "@tetherto/wdk-wallet-evm": "^1.0.0-beta.2",
    "@tetherto/wdk-wallet-evm-erc-4337": "^1.0.0-beta.1",
    "bare-node-runtime": "1.1.1",
    "ethers": "6.14.3"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "jest": "29.7.0",
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bare": "./bare.js",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "standard": {
    "ignore": [
      "bare.js",
      "tests/**/*.js"
    ]
  }
}
