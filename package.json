{
  "name": "@wdk/protocol-lending-aave-evm",
  "version": "1.0.0-beta.1",
  "description": "A simple package that lets @wdk/wallet-evm and wallet-evm-erc-4337 wallet accounts interact with the aave protocol.",
  "keywords": ["wdk", "protocol", "lending", "aave", "ethereum", "erc-4337"],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/tetherto/wdk-protocol-lending-aave-evm.git"
  },
  "main": "index.js",
  "type": "module",
  "types": "./types",
  "scripts": {
    "build:types": "tsc",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "dependencies": {
    "@bgd-labs/aave-address-book": "4.29.3",
    "@wdk/wdk-wallet": "https://github.com/davi0kprogramsthings/wdk-wallet#develop",
    "@wdk/wdk-wallet-evm": "https://github.com/davi0kprogramsthings/wdk-wallet-evm#develop",
    "@wdk/wdk-wallet-evm-erc-4337": "https://github.com/davi0kprogramsthings/wdk-wallet-evm-erc-4337#develop",
    "bare-wdk-runtime": "2.0.0",
    "ethers": "6.14.3"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "jest": "29.7.0",
    "standard": "17.1.2",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bare": "./bare.js",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  },
  "standard": {
    "ignore": [
      "bare.js",
      "tests/**/*.js"
    ]
  }
}
